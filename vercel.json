{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",   
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "45mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
      "FLASK_APP": "api/index.py"
  }
}